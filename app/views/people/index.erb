<h1>People</h1>
<table>
  <thead>
    <th>Name</th>
    <th>Birthdate</th>
  </thead>
  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td>
          <a href="<%= "/people/#{person.id} %>">"
            <%= "#{person.first_name} #{person.last_name}" %>
          </a>
        </td>
        <td>
          <%= "#{person.birthdate}" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>







<ul>
  <% @people.each do |person| %>
    <li><%= "#{person.first_name} #{person.last_name} - #{person.birthdate}" %></li>
  <% end %>
</ul>
